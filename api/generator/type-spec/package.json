{
  "name": "type-spec-learn",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "npm run format && tsp compile .",
    "format": "tsp format \"**/*.tsp\"",
    "format:check": "tsp format --check \"**/*.tsp\"",
    "test": "npm run format:check"
  },
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/rest": "latest",
    "@typespec/openapi3": "latest"
  },
  "private": true,
  "volta": {
    "node": "20.12.2",
    "npm": "10.6.0"
  }
}